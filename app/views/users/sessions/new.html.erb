<div class="login1">
  <video autoplay="autoplay" loop="loop" muted="muted" class="video-content" 			src="https://webukatu.com/wordpress/wp-content/uploads/2020/11/Creux-De-Van-45150.mp4"></video>

<div class="login2">

<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <br />
    <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
  </div>

  <div class="field">
    <br />
    <%= f.password_field :password, placeholder: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

</div>
</div>